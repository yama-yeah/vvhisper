{
  "name": "icon",
  "description": "A flutter chrome extension",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "minimum_chrome_version": "10.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "back.dart.js"
  },
  "host_permissions": [
    "https://*/*",
    "https://hope.fun.ac.jp/*"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "notifications",
    "webRequest",
    "nativeMessaging",
    "webNavigation",
    "cookies"
  ]
}